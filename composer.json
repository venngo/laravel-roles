{
    "name": "venngo/laravel-roles",
    "description": "A Powerful package for handling roles and permissions in Laravel. Supports Laravel 5.3 up to 12.",
    "keywords": [
        "laravel-roles",
        "laravel-permissions",
        "laravel-levels",
        "roles",
        "permissions",
        "acl",
        "levels",
        "laravel"
    ],
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "Roman Biƒçan",
            "email": "romanbican@seznam.cz"
        },
        {
            "name": "Jeremy Kenedy",
            "email": "jeremykenedy@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0|^8.1|^8.2|^8.3|^8.4",
        "eklundkristoffer/seedster": "^8.0|^9.0",
        "laravel/helpers": "^1.7"
    },
    "require-dev": {
        "illuminate/support": "^8.5|^9.0|^10.0|^11.0|^12.0",
        "laravel/framework": "12.1.1",
        "laravel/laravel": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "laravel/tinker": "^2.9",
        "orchestra/testbench": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "jeremykenedy\\LaravelRoles\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "jeremykenedy\\LaravelRoles\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "jeremykenedy\\LaravelRoles\\RolesServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
